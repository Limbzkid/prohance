      <div class="copy flex-wrp">
        <p class="copyright">Copyright &copy; Sun Pharmaceutical Industries Ltd. </p>
        <ul class="flex-wrp">
          {{ page.footer }}
        </ul>
      </div>
      <div class="subs-wrp">
        <ul class="flex-wrp">
          <li><a href="javascript:;">subscribe</a></li>
          <li><a href="javascript:;">unbscribe</a></li>
        </ul>
      </div>
      <div class="social-wrp">
        <ul class="flex-wrp">
			<li><a href="https://www.facebook.com/Sun-Pharma-170795602978664/" target="_blank"><img src="/{{ directory }}/images/fb.png" alt="fb"></a></li>
			<li><a href="https://twitter.com/SunPharma_live" target="_blank"><img src="/{{ directory }}/images/tw.png" alt="tw"></a></li>
			<li><a href="https://www.youtube.com/channel/UCdkyfWmDFAaUA5P8GD_LVdQ" target="_blank"><img src="/{{ directory }}/images/yt.png" alt="yt"></a></li>
